T = float(input("the temp is: ")) #Temparature
H = float(input("the Humidity is: ")) #Humidity
W = float(input("the wind speed is:")) #Wind speed

w = (0.5 * T * T - 0.2 * H + 0.1 * W - 15)
print(w)

if w > 300:
    print("the weather is sunny")
elif 200 < w <= 300:
    print("the weather is cloudy")
elif 100 < w <= 200:
    print("the weather is rainy")
else:
    print("the weather is stormy")
